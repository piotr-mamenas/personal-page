{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "client",
      "image": "personal-page/client",
      "hostname": "client",
      "essential": false,
      "memory": 128
    },
    {
      "name": "endpoint",
      "image": "personal-page/endpoint",
      "hostname": "endpoint",
      "essential": false,
      "memory": 128
    },
    {
      "name": "nginx",
      "image": "personal-page/nginx",
      "hostname": "nginx",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": ["client","endpoint"],
      "memory": 256
    }
  ]
}
